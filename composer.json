{
    "name": "easybib/api-client-php",
    "description": "Client for EasyBib API, implemented in PHP",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Yitzchak Schaffer",
            "email": "yitzchak@imagineeasy.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:fabpot/php-cs-fixer.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:fkooman/guzzle-bearer-auth-plugin"
        },
        {
            "type": "vcs",
            "url": "git@github.com:yitznewton/php-oauth-client"
        }
    ],
    "require": {
        "php": ">=5.5",
        "guzzle/guzzle": "3.8.*",
        "fkooman/guzzle-bearer-auth-plugin": "dev-master",
        "fkooman/php-oauth-client": "dev-master"
    },
    "require-dev": {
      "phpunit/phpunit": ">=3.7",
      "phpmd/phpmd": "*",
      "fabpot/php-cs-fixer": "dev-master",
      "squizlabs/php_codesniffer": "1.*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "EasyBib\\Api\\Client\\": "src/",
            "EasyBib\\Tests\\Api\\Client\\": "tests/"
        },
        "classmap": ["tests/"]
    }
}
